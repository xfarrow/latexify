{
  "manifest_version": 2,
  "name": "Latexify",
  "version": "1.0",
  
  "description": "Allows to insert LaTeX elements visually",
  
    "icons": {
    "48": "icons/logo.png"
  },
  
    "permissions": [
    "activeTab"
  ],

    "browser_action": {
    "default_icon": "icons/logo_32.png",
    "default_title": "Latexify",
    "default_popup": "popup/popup.html"
  },

}
